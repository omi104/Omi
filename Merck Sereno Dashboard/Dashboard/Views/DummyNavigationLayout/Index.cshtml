@model Dashboard.Models.Config.DummyNavigationLayoutConfig

<div id="snapShotWidget" style="width: 100%; padding-left: 1%;">
    <div style="width: 99%; display: inline-block; margin: 0;" class="row">
        <div class="col-lg-12">
            <ol class="breadcrumb">
                <li class="active">This view displays Reckit Benkiser's Company ranking in comparisons to the competitors.
                </li>
            </ol>
        </div>
    </div>

    <div style="width: 99%; display: inline-block; margin: 0;" class="row">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
            <div style="width: 560px;" class="WidgetHeading panel-heading">
                <h4 class="panel-title">Dataview</h4>
                <div class="panel-controls" style="float: right">
                    <a href="javascript:void(0)" class="toggle panel-minimize"><i class="fa fa-minus"></i></a>
                    <a href="javascript:void(0)" class="panel-close"><i class="fa fa-close"></i></a>
                </div>
            </div>
            <div class="panel-body">
                <div style="width: 100%;" id="widget-CompanyTopSnapshotTable-view" />
            </div>
        </div>
    </div>

    <div style="width: 99%; display: inline-block; margin: 0; padding-left: 1%;" class="row" />
    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
        <div style="width: 560px;" class="WidgetHeading panel-heading">
            <h4 class="panel-title">Pie Chart</h4>
            <div class="panel-controls" style="float: right">
                <a href="#" class="toggle panel-minimize"><i class="fa fa-minus"></i></a>
                <a href="#" class="panel-close"><i class="fa fa-close"></i></a>
            </div>
            @*<i class="fa fa-caret-down sideNav-arrow"></i>*@
        </div>
        <div class="panel-body">
            <div id="widget-CompanySnapshotChart-view" />
        </div>
    </div>

    <div style="width: 99%; display: inline-block; margin: 0;" class="row recordCount">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
            <div id="filter-RecordCount-layout"></div>
        </div>
    </div>
    <div style="width: 99%; display: inline-block; margin: 0;" class="row">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
            <div style="width: 100%;" id="widget-CompanySnapshotTable-view" />
        </div>
    </div>
</div>

